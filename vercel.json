{
    "builds": [
     {
         "src":"src/index.js",
         "use": "@vercel/node"
     }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "src/index.js" },
        { "src": "/img/(.*)", "dest": "/public/img/$1" },
        { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
        { "src": "/js/(.*)", "dest": "/public/js/$1" },
        { "src": "/favicon.ico", "dest": "/public/favicon.ico" }
    ]
}